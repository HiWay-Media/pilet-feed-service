{
  "name": "sample-pilet-service",
  "version": "1.5.2",
  "description": "Piral: Sample pilet feed service.",
  "main": "dist",
  "typings": "dist",
  "homepage": "https://piral.io",
  "scripts": {
    "test:unit": "jest src",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run lint && npm run test:unit",
    "start": "node dist/cli.js",
    "watch": "node index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.ts'",
    "build": "tsc --project tsconfig.json",
    "serve": "pm2-runtime dist/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "sample-pilet-service": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:smapiot/sample-pilet-service.git"
  },
  "keywords": [
    "piral",
    "pilet",
    "feed",
    "service",
    "sample"
  ],
  "author": {
    "name": "smapiot",
    "url": "https://smapiot.com"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.25.3",
    "connect-busboy": "^0.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "graphql": "^14.7.0",
    "graphql-type-json": "^0.3.2",
    "mime-types": "^2.1.34",
    "pm2": "^5.1.2",
    "response-time": "^2.3.2",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@types/connect-busboy": "^0.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/graphql-type-json": "^0.3.0",
    "@types/jest": "^25.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^11.15.54",
    "@types/response-time": "^2.3.5",
    "@types/tar": "^4.0.5",
    "dotenv": "^8.6.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.9.10"
  }
}
